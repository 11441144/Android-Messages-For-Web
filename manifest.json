{
  "name": "Android Messages For Web",
  "version": "1.6",
  "manifest_version": 2,
  "minimum_chrome_version": "37.0.0.0",
  "permissions": [
    "webview",
    "notifications",
    "storage",
    "system.display",
    {
      "fileSystem": [
        "write",
        "retainEntries",
        "directory"
      ]
    },
    "accessibilityFeatures.read"
  ],
  "app": {
    "background": {
      "scripts": [
        "js\/foam.js",
        "js\/cab.js",
        "js\/window.js",
        "config.js",
        "background_main.js"
      ]
    }
  },
  "default_locale": "en",
  "description": "Unofficial Chrome app created to make it easier for ChromeOS users to access Android messages.",
  "icons": {
    "256": "img\/256.png",
    "128": "img\/128.png",
    "96": "img\/96.png",
    "64": "img\/64.png",
    "48": "img\/48.png",
    "32": "img\/32.png",
    "16": "img\/16.png"
  }
}